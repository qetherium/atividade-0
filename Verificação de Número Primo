#include <stdio.h>

int main(){
    long int n;
    int primo = 1;

    scanf("%ld", &n);

    if(n == 0 || n == 1){
        primo = 0;
    }
    else if(n == 2){
        primo;
    }
    else if(n != 2 && n%2 == 0){
        primo = 0;
    }
    else{
        for(int i=3; i<n; i++){
            if(n%i == 0){
                primo = 0;
                break;
            }
        }
    }

    if(primo) printf("%ld é primo\n", n);
    else printf("%ld não é primo\n", n);

    return 0;
}
